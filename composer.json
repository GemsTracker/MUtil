{
    "name": "magnafacta/mutil",
    "description": "MagnaFacta Utilities for site development",
    "license": "BSD-3-Clause",
    "type": "library",
    "homepage": "http://magnafacta.info/",
    "authors": [
        {
            "name": "Matijs de Jong",
            "email": "mjong@magnafacta.nl"
        },
        {
            "name": "Menno Dekker",
            "email": "menno.dekker@erasmusmc.nl"
        },
        {
            "name": "Jasper van Gestel",
            "email": "jvangestel@gmail.com"
        },
        {
            "name": "Frank Santegoets",
            "email": "f.santegoets@erasmusmc.nl"
        },
        {
            "name": "Roel Faber",
            "email": "r.faber@erasmusmc.nl"
        },
        {
            "name": "Michiel Rook",
            "email": "mrook@php.net"
        }
    ],
    "autoload": {
        "psr-0": {
            "MUtil_": "src/"
        },
        "psr-4": {
            "MUtil\\": "src/MUtil"
        }
    },
    "include-path": [
        "src/"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "psr/http-message": "^1.0.1",
        "shardj/zf1-future": "^1.21",
        "shardj/zf1-extras-future": "^1.12",
        "psr/cache": "^3.0",
        "symfony/cache-contracts": "^3.1"
    },
    "require-dev": {
        "phing/phing": "^2.17.3"
    },
    "autoload-dev": {
        "psr-0": {
            "MUtil_": "tests/"
        },
        "psr-4": {
            "MUtil\\": "tests/MUtil"
        }
    },
    "archive": {
        "exclude": ["/tests"]
    }
}
