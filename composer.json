{
    "name": "magnafacta/mutil",
    "description": "MagnaFacta Utilities for site development",
    "license": "BSD-3-Clause",
    "type": "library",
    "homepage": "http://magnafacta.info/",
    "authors": [
        {
            "name": "Matijs de Jong",
            "email": "mjong@magnafacta.nl"
        },
        {
            "name": "Menno Dekker",
            "email": "menno.dekker@erasmusmc.nl"
        },
        {
            "name": "Jasper van Gestel",
            "email": "jvangestel@gmail.com"
        },
        {
            "name": "Frank Santegoets",
            "email": "f.santegoets@erasmusmc.nl"
        },
        {
            "name": "Roel Faber",
            "email": "r.faber@erasmusmc.nl"
        },
        {
            "name": "Michiel Rook",
            "email": "mrook@php.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "MUtil\\": "src/MUtil"
        }
    },
    "include-path": [
        "src/"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "laminas/laminas-validator": "^2.20",
        "magnafacta/zalt-html": "^1.0.1",
        "magnafacta/zalt-model": "^2.0.2",
        "psr/http-message": "^1.0.1",
        "psr/cache": "^3.0",
        "shardj/zf1-future": "^1.21",
        "shardj/zf1-extras-future": "^1.12",
        "symfony/translation": "^6.1",
        "symfony/finder": "^6.1"
    },
    "require-dev": {
        "phing/phing": "^2.17.3",
        "phpunit/phpunit": "10.*",
        "phpspec/prophecy-phpunit": "^2.0",
        "symfony/yaml": "^6.1"
    },
    "autoload-dev": {
        "psr-4": {
            "MUtilTest\\": "tests"
        }
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/MagnaFacta/zalt-html.git"
        },
        {
            "type": "git",
            "url": "https://github.com/MagnaFacta/zalt-late.git"
        },
        {
            "type": "git",
            "url": "https://github.com/MagnaFacta/zalt-loader.git"
        },
        {
            "type": "git",
            "url": "https://github.com/MagnaFacta/zalt-model.git"
        },
        {
            "type": "git",
            "url": "https://github.com/MagnaFacta/zalt-util.git"
        }
    ],
    "scripts": {
        "test": [
            "vendor/bin/phpunit tests"
        ]
     }
}
